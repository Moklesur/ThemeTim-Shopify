<!doctype html>
<!--[if IE 8]><html class="no-js lt-ie9" lang="en"> <![endif]-->
<!--[if IE 9 ]><html class="ie9 no-js"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> <html class="no-js"> <!--<![endif]-->
<head>
  <!--------------- Basic page needs ---------------->
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <!--------------- Description ---------------->
  {% if page_description %}
  <meta name="description" content="{{ page_description | escape }}">
  {% endif %}

  <!--------------- Favicon ---------------->
  <link rel="shortcut icon" href="{{ 'favicon.png' | asset_url }}" type="image/png" />

  <!--------------- Title and description ---------------->
  <title>
  {{ page_title }}{% if current_tags %}{% assign meta_tags = current_tags | join: ', ' %} &ndash; {{ 'general.meta.tags' | t: tags: meta_tags }}{% endif %}{% if current_page != 1 %} &ndash; {{ 'general.meta.page' | t: page: current_page }}{% endif %}{% unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless %}
  </title>

  <!--------------- Style ---------------->
  {{ 'font-awesome.min.css' | asset_url | stylesheet_tag }}
  {{ 'animate.min.css' | asset_url | stylesheet_tag }}
  {{ 'owl.carousel.css' | asset_url | stylesheet_tag }}
  {{ 'bootstrap.min.css' | asset_url | stylesheet_tag }}
  {{ 'style.css' | asset_url | stylesheet_tag }}
  {{ 'themetim.css' | asset_url | stylesheet_tag }}
  {{ 'mobile.css' | asset_url | stylesheet_tag }}

  <!--------------- Font Family ---------------->
  <link href='https://fonts.googleapis.com/css?family=Roboto:300,400,700' rel='stylesheet' type='text/css'>
  {% if settings.paragraph_font contains 'Google' %}
  <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family={{ settings.paragraph_font | remove: 'Google+' }}:300,400">
  {% endif %}

  <!--------------- Header hook for plugins ---------------->
  {{ content_for_header }}

  <!--------------- JavaScript ---------------->
  {{ 'jquery-1.11.2.min.js' | asset_url | script_tag }}
  {{ 'jquery-migrate-1.2.1.js' | asset_url | script_tag }}

  <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
  <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
  <!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
  <![endif]-->

  {% comment %}
    If you store has customer accounts disabled, you can remove the following JS file
  {% endcomment %}
  {% if template contains 'customers' %}
    {{ 'shopify_common.js' | shopify_asset_url | script_tag }}
    {{ 'customer_area.js'  | shopify_asset_url | script_tag }}
  {% endif %}

</head>

<body class="body-container">

    <!--------------- Header ---------------->
    {% include 'header' %}
    <!--------------- Breadcrumb ---------------->
    {% include 'breadcrumb' %}
    <!--------------- Main Content ---------------->
    {{ content_for_layout }}
    <!--------------- Footer ---------------->
    {% include 'footer' %}

    <!--------------- Welcome Newsletter ---------------->
    <div class="modal fade welcome-newsletter-content " id="welcome-newsletter" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="false">
      <div class="modal-dialog modal-md">
        <div class="modal-content">
          <div class="modal-body">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
            <div class="welcome-newsletter text-center">
              <div class="text-center">
                <h3 class="text-uppercase margin-clear padding-top-10">Newsletter</h3>
              </div>
              <div class="">
                <p class="text-capitalize rale-light margin-top-5 margin-bottom-0">Nunc Vero A Primo Quidem Mirabiliter Occulta Natura Est Nec Perspi Nec Cognosci Potest. Cum Sciret</p>
              </div>

              <form action="#" class="margin-bottom-10" method="post" target="_blank">
                <input type="email" value="" name="EMAIL" placeholder="Email Address" required="" class="form-control margin-bottom-20 full-width">
                <button type="submit" value="Send" name="subscribe" class="btn btn-default margin-bottom-20 btn-lg text-uppercase">Send</button>
              </form>
              <ul class="list-inline margin-top-20">
                <li class="margin-top-10"><a href="#" target="_blank"><i class="fa fa-facebook fa-2x"></i></a></li>
                <li class="margin-top-10"><a href="#" target="_blank"><i class="fa fa-twitter fa-2x"></i></a></li>
                <li class="margin-top-10"><a href="#" target="_blank"><i class="fa fa-linkedin fa-2x"></i></a></li>
                <li class="margin-top-10"><a href="#" target="_blank"><i class="fa fa-google-plus fa-2x"></i></a></li>
              </ul>
            </div>
          </div>
        </div>
      </div>
    </div>

    <!--------------- Add To Cart ---------------->
    <div class="modal fade myModal" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-body">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">×</span>
            </button>
            <div class="row">
              <div class="col-md-3 paira-success-message-img margin-bottom-20">
                <img src="http://placehold.it/1024x600" class="img-responsive">
              </div>
              <div class="col-md-9 margin-bottom-20">
                <h4 class="margin-bottom-15 paira-success-message-title">Product Title</h4>
              </div>
            </div>
            <div class="row">
              <div class="col-md-12">
                <div class="alert alert-success margin-bottom-0 margin-top-15 padding-top-5 padding-bottom-5 text-center" role="alert"><strong class="paira-success-message-details"><i class="fa fa-check"></i> Added To Your Shopping Cart.</strong></div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>

    <!--------------- JavaScript ---------------->
    {{ 'jquery.cookie.js' | asset_url | script_tag }}
    {{ 'bootstrap.min.js' | asset_url | script_tag }}
    {{ 'jquery.easing.min.js' | asset_url | script_tag }}
    {{ 'owl.carousel.min.js' | asset_url | script_tag }}
    {{ 'jquery.parallax-1.1.3.js' | asset_url | script_tag }}
    {{ 'option_selection.js' | shopify_asset_url | script_tag }}
    {{ 'api.jquery.js' | shopify_asset_url | script_tag }}
    {{ 'script.js' | asset_url | script_tag }}

    <script>
      $(function() {
        $('.home-parallax').parallax("50%", 0.3);
      });
    </script>

</body>
</html>
